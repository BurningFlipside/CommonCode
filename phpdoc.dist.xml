<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <parser>
    <target>./docs</target>
  </parser>
  <transformer>
    <target>./docs</target>
  </transformer>
  <files>
    <directory>.</directory>
    <ignore>tests/*</ignore>
    <ignore>docs/*</ignore>
    <ignore>vendor/*</ignore>
    <ignore>libs/*</ignore>
    <ignore>js/*</ignore>
  </files>
</phpdoc>
